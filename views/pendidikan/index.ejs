<%- include('../partials/header') %>

    <div class="container">
        <%- include('../partials/message') %>

        <div class="row">
            <div class="col">
                <a href="/pendidikan/create" class="btn btn-sm btn-success">Tambah</a>
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Nama Instansi</th>
                            <th scope="col">Jurusan</th>
                            <th scope="col">Tahun Masuk</th>
                            <th scope="col">Tahun Lulus</th>
                            <th scope="col">Nomor Ijazah</th>
                            <th scope="col">ID Mahasiswa</th>
                            <th colspan="2" scope="col">Aksi</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for (var i = 0; i < data.length; i++) { %>
                        <tr>
                            <th scope="row"><%= (i + 1) %></th>
                            <td><%= data[i].nama_instansi %></td>
                            <td><%= data[i].jurusan %></td>
                            <td><%= data[i].tahun_masuk %></td>
                            <td><%= data[i].tahun_lulus %></td>
                            <td><%= data[i].nomor_ijazah %></td>
                            <td><%= data[i].id_mahasiswa %></td>
                            <td>
                                <a href="/pendidikan/edit/<%= data[i].id %>"
                                    class="btn btn-sm btn-warning">Edit</a>
                            </td>
                            <td>
                                <a href="/pendidikan/delete/<%= data[i].id %>"
                                    class="btn btn-sm btn-danger">Delete</a>
                            </td>
                        </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
<%- include('../partials/header') %>
      
      